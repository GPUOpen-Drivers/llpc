#version 450 core

layout(set = 0, binding = 0) uniform samplerBuffer samp;

layout(set = 0, binding = 1) buffer BO
{
    vec4 mOut;
};

void main()
{
    vec4 temp1 = texelFetch(samp, 3);
    mOut = temp1;
}

